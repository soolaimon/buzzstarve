{
	"manifest_version": 2,

	"name": "Buzzstarve",
	"description": "Blocks Facebook posts linking to buzzefeed.com.",
	"version": "1.0",

	"permissions": ["*://*.facebook.com/*" ],

	"content_scripts": [
		{
			"matches": [ "*://*.facebook.com/*" ],
			"js": [ "jquery-2.0.3.min.js", "block.js" ]
		}

	]
}